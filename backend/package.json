{
  "name": "auto-grade-backend",
  "version": "1.0.0",
  "description": "Backend for Auto-Grade system with OCR and ML capabilities",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint src/",
    "seed": "node scripts/seed.js",
    "setup-ocr": "cd ml && pip install -r requirements.txt",
    "test:watch": "jest --watch",
    "test:teacher-portal": "jest tests/teacher-portal.test.js",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "setup": "./setup-enhanced-teacher-portal.sh",
    "cleanup": "./scripts/cleanup-teacher-portal.sh",
    "backup": "./scripts/backup-teacher-data.sh",
    "monitor": "./scripts/monitor-teacher-portal.sh",
    "migrate": "node scripts/migrate.js"
  },
  "dependencies": {
    "archiver": "^5.3.2",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.5",
    "nodemailer": "^6.10.1",
    "pdf-parse": "^1.1.1",
    "sequelize": "^6.37.7",
    "sharp": "^0.32.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4"
  }
}